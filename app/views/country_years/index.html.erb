<h1>Listing country_years</h1>

<table>
  <tr class="tabhead">
	<th>Country</th>
    <th>Year</th>
    <th>Emission</th>
    <th>Sufficiency</th>
    <th>Area habitats</th>
    <th>Area habitats percent</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @country_years.each do |country_year| %>
  <tr class="<%= cycle("tabeven","tabodd") %>">
	<td><%= country_year.country.name%>
    <td><%= country_year.number %></td>
    <td><%= country_year.emission %></td>
    <td><%= country_year.sufficiency %></td>
    <td><%= country_year.area_habitats_scaled %></td>
    <td><%= country_year.area_habitats_percent %></td>
    <td><%= link_to 'Show', country_year %></td>
    <td><%= link_to 'Edit', edit_country_year_path(country_year) %></td>
    <td><%= link_to 'Destroy', country_year, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Country year', new_country_year_path %>
